<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">

    <browser:page
        name="sharing"
        for="*"
        class=".sharing.SharingView"
        permission="cmf.ChangeLocalRoles"
        />

    <browser:page
        name="prefs_types"
        for="*"
        class=".prefs_types.PrefsTypesView"
        permission="cmf.ChangeLocalRoles"
        />
        
</configure>
